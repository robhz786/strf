                                    if (ch <= 0x16F87) {    // 16F51 .. 16F87
                                        goto handle_spacing_mark;
                                    } else {                // 16F88 .. 16F92
                                        if (ch <= 0x16F8E) {// 16F88 .. 16F8E
                                            goto handle_other;
                                        } else {            // 16F8F .. 16F92
                                            goto handle_extend;
                                        }
                                    }
                                }
                            }
                        }
                    } else {                                // 16F93 .. 1D9FF
                        if (ch <= 0x1D172) {                // 16F93 .. 1D172
                            if (ch <= 0x1BC9F) {            // 16F93 .. 1BC9F
                                if (ch <= 0x16FF1) {        // 16F93 .. 16FF1
                                    if (ch <= 0x16FEF) {    // 16F93 .. 16FEF
                                        if (ch == 0x16FE4)
                                            goto handle_extend;
                                        goto handle_other;
                                    } else {                // 16FF0 .. 16FF1
                                        goto handle_spacing_mark;
                                    }
                                } else {                    // 16FF2 .. 1BC9F
                                    if (ch <= 0x1BC9C) {    // 16FF2 .. 1BC9C
                                        goto handle_other;
                                    } else {                // 1BC9D .. 1BC9F
                                        if (ch == 0x1BC9F)
                                            goto handle_other;
                                        goto handle_extend;
                                    }
                                }
                            } else {                        // 1BCA0 .. 1D172
                                if (ch <= 0x1D166) {        // 1BCA0 .. 1D166
                                    if (ch <= 0x1BCA3) {    // 1BCA0 .. 1BCA3
                                        goto handle_control;
                                    } else {                // 1BCA4 .. 1D166
                                        if (ch == 0x1D165)
                                            goto handle_extend;
                                        if (ch == 0x1D166)
                                            goto handle_spacing_mark;
                                        goto handle_other;
                                    }
                                } else {                    // 1D167 .. 1D172
                                    if (ch <= 0x1D169) {    // 1D167 .. 1D169
                                        goto handle_extend;
                                    } else {                // 1D16A .. 1D172
                                        if (ch <= 0x1D16D) {// 1D16A .. 1D16D
                                            if (ch == 0x1D16D)
                                                goto handle_spacing_mark;
                                            goto handle_other;
                                        } else {            // 1D16E .. 1D172
                                            goto handle_extend;
                                        }
                                    }
                                }
                            }
                        } else {                            // 1D173 .. 1D9FF
                            if (ch <= 0x1D18B) {            // 1D173 .. 1D18B
                                if (ch <= 0x1D182) {        // 1D173 .. 1D182
                                    if (ch <= 0x1D17A) {    // 1D173 .. 1D17A
                                        goto handle_control;
                                    } else {                // 1D17B .. 1D182
                                        goto handle_extend;
                                    }
                                } else {                    // 1D183 .. 1D18B
                                    if (ch <= 0x1D184) {    // 1D183 .. 1D184
                                        goto handle_other;
                                    } else {                // 1D185 .. 1D18B
                                        goto handle_extend;
                                    }
                                }
                            } else {                        // 1D18C .. 1D9FF
                                if (ch <= 0x1D1AD) {        // 1D18C .. 1D1AD
                                    if (ch <= 0x1D1A9) {    // 1D18C .. 1D1A9
                                        goto handle_other;
                                    } else {                // 1D1AA .. 1D1AD
                                        goto handle_extend;
                                    }
                                } else {                    // 1D1AE .. 1D9FF
                                    if (ch <= 0x1D241) {    // 1D1AE .. 1D241
                                        goto handle_other;
                                    } else {                // 1D242 .. 1D9FF
                                        if (ch <= 0x1D244) {// 1D242 .. 1D244
                                            goto handle_extend;
                                        } else {            // 1D245 .. 1D9FF
                                            goto handle_other;
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            } else {                                        // 1DA00 ..10FFFF
                if (ch <= 0x1F3FA) {                        // 1DA00 .. 1F3FA
                    if (ch <= 0x1EFFF) {                    // 1DA00 .. 1EFFF
                        if (ch <= 0x1E02A) {                // 1DA00 .. 1E02A
                            if (ch <= 0x1DA9A) {            // 1DA00 .. 1DA9A
                                if (ch <= 0x1DA3A) {        // 1DA00 .. 1DA3A
                                    if (ch <= 0x1DA36) {    // 1DA00 .. 1DA36
                                        goto handle_extend;
                                    } else {                // 1DA37 .. 1DA3A
                                        goto handle_other;
                                    }
                                } else {                    // 1DA3B .. 1DA9A
                                    if (ch <= 0x1DA6C) {    // 1DA3B .. 1DA6C
                                        goto handle_extend;
                                    } else {                // 1DA6D .. 1DA9A
                                        if (ch == 0x1DA75)
                                            goto handle_extend;
                                        if (ch == 0x1DA84)
                                            goto handle_extend;
                                        goto handle_other;
                                    }
                                }
                            } else {                        // 1DA9B .. 1E02A
                                if (ch <= 0x1DFFF) {        // 1DA9B .. 1DFFF
                                    if (ch <= 0x1DAAF) {    // 1DA9B .. 1DAAF
                                        if (ch == 0x1DAA0)
                                            goto handle_other;
                                        goto handle_extend;
                                    } else {                // 1DAB0 .. 1DFFF
                                        goto handle_other;
                                    }
                                } else {                    // 1E000 .. 1E02A
                                    if (ch <= 0x1E018) {    // 1E000 .. 1E018
                                        if (ch == 0x1E007)
                                            goto handle_other;
                                        goto handle_extend;
                                    } else {                // 1E019 .. 1E02A
                                        if (ch <= 0x1E01A) {// 1E019 .. 1E01A
                                            goto handle_other;
                                        } else {            // 1E01B .. 1E02A
                                            if (ch == 0x1E022)
                                                goto handle_other;
                                            if (ch == 0x1E025)
                                                goto handle_other;
                                            goto handle_extend;
                                        }
                                    }
                                }
                            }
                        } else {                            // 1E02B .. 1EFFF
                            if (ch <= 0x1E2EF) {            // 1E02B .. 1E2EF
                                if (ch <= 0x1E136) {        // 1E02B .. 1E136
                                    if (ch <= 0x1E12F) {    // 1E02B .. 1E12F
                                        goto handle_other;
                                    } else {                // 1E130 .. 1E136
                                        goto handle_extend;
                                    }
                                } else {                    // 1E137 .. 1E2EF
                                    if (ch <= 0x1E2EB) {    // 1E137 .. 1E2EB
                                        goto handle_other;
                                    } else {                // 1E2EC .. 1E2EF
                                        goto handle_extend;
                                    }
                                }
                            } else {                        // 1E2F0 .. 1EFFF
                                if (ch <= 0x1E8D6) {        // 1E2F0 .. 1E8D6
                                    if (ch <= 0x1E8CF) {    // 1E2F0 .. 1E8CF
                                        goto handle_other;
                                    } else {                // 1E8D0 .. 1E8D6
                                        goto handle_extend;
                                    }
                                } else {                    // 1E8D7 .. 1EFFF
                                    if (ch <= 0x1E943) {    // 1E8D7 .. 1E943
                                        goto handle_other;
                                    } else {                // 1E944 .. 1EFFF
                                        if (ch <= 0x1E94A) {// 1E944 .. 1E94A
                                            goto handle_extend;
                                        } else {            // 1E94B .. 1EFFF
                                            goto handle_other;
                                        }
                                    }
                                }
                            }
                        }
                    } else {                                // 1F000 .. 1F3FA
                        if (ch <= 0x1F19A) {                // 1F000 .. 1F19A
                            if (ch <= 0x1F16B) {            // 1F000 .. 1F16B
                                if (ch <= 0x1F10C) {        // 1F000 .. 1F10C
                                    if (ch <= 0x1F0FF) {    // 1F000 .. 1F0FF
                                        goto handle_extended_picto;
                                    } else {                // 1F100 .. 1F10C
                                        goto handle_other;
                                    }
                                } else {                    // 1F10D .. 1F16B
                                    if (ch <= 0x1F10F) {    // 1F10D .. 1F10F
                                        goto handle_extended_picto;
                                    } else {                // 1F110 .. 1F16B
                                        if (ch == 0x1F12F)
                                            goto handle_extended_picto;
                                        goto handle_other;
                                    }
                                }
                            } else {                        // 1F16C .. 1F19A
                                if (ch <= 0x1F17D) {        // 1F16C .. 1F17D
                                    if (ch <= 0x1F171) {    // 1F16C .. 1F171
                                        goto handle_extended_picto;
                                    } else {                // 1F172 .. 1F17D
                                        goto handle_other;
                                    }
                                } else {                    // 1F17E .. 1F19A
                                    if (ch <= 0x1F17F) {    // 1F17E .. 1F17F
                                        goto handle_extended_picto;
                                    } else {                // 1F180 .. 1F19A
                                        if (ch <= 0x1F190) {// 1F180 .. 1F190
                                            if (ch == 0x1F18E)
                                                goto handle_extended_picto;
                                            goto handle_other;
                                        } else {            // 1F191 .. 1F19A
                                            goto handle_extended_picto;
                                        }
                                    }
                                }
                            }
                        } else {                            // 1F19B .. 1F3FA
                            if (ch <= 0x1F20F) {            // 1F19B .. 1F20F
                                if (ch <= 0x1F1E5) {        // 1F19B .. 1F1E5
                                    if (ch <= 0x1F1AC) {    // 1F19B .. 1F1AC
                                        goto handle_other;
                                    } else {                // 1F1AD .. 1F1E5
                                        goto handle_extended_picto;
                                    }
                                } else if (ch <= 0x1F1FF) { // 1F1E6 .. 1F1FF
                                    goto handle_regional_indicator;
                                } else if (ch == 0x1F200) {
                                    goto handle_other;
                                } else  {                   // 1F201 .. 1F20F
                                    goto handle_extended_picto;
                                }
                            } else {                        // 1F210 .. 1F3FA
                                if (ch <= 0x1F23F) {        // 1F210 .. 1F23F
                                    if (ch <= 0x1F231) {    // 1F210 .. 1F231
                                        if (ch == 0x1F21A)
                                            goto handle_extended_picto;
                                        if (ch == 0x1F22F)
                                            goto handle_extended_picto;
                                        goto handle_other;
                                    } else {                // 1F232 .. 1F23F
                                        if (ch == 0x1F23B)
                                            goto handle_other;
                                        goto handle_extended_picto;
                                    }
                                } else {                    // 1F240 .. 1F3FA
                                    if (ch <= 0x1F248) {    // 1F240 .. 1F248
                                        goto handle_other;
                                    } else {                // 1F249 .. 1F3FA
                                        if (ch <= 0x1F2FF) {// 1F249 .. 1F2FF
                                            goto handle_extended_picto;
                                        } else {            // 1F300 .. 1F3FA
                                            ch_width = 2_w;
                                            goto handle_extended_picto;
                                        }
                                    }
                                }
                            }
                        }
                    }
                } else {                                    // 1F3FB ..10FFFF
                    if (ch <= 0x1F88F) {                    // 1F3FB .. 1F88F
                        if (ch <= 0x1F7D4) {                // 1F3FB .. 1F7D4
                            if (ch <= 0x1F64F) {            // 1F3FB .. 1F64F
                                ch_width = 2_w;
                                if (ch <= 0x1F53D) {        // 1F3FB .. 1F53D
                                    if (ch <= 0x1F3FF) {    // 1F3FB .. 1F3FF
                                        goto handle_extend;
                                    } else {                // 1F400 .. 1F53D
                                        goto handle_extended_picto;
                                    }
                                } else {                    // 1F53E .. 1F64F
                                    if (ch <= 0x1F545) {    // 1F53E .. 1F545
                                        goto handle_other;
                                    } else {                // 1F546 .. 1F64F
                                        goto handle_extended_picto;
                                    }
                                }
                            } else {                        // 1F650 .. 1F7D4
                                if (ch <= 0x1F6FF) {        // 1F650 .. 1F6FF
                                    if (ch <= 0x1F67F) {    // 1F650 .. 1F67F
                                        goto handle_other;
                                    } else {                // 1F680 .. 1F6FF
                                        goto handle_extended_picto;
                                    }
                                } else {                    // 1F700 .. 1F7D4
                                    if (ch <= 0x1F773) {    // 1F700 .. 1F773
                                        goto handle_other;
                                    } else {                // 1F774 .. 1F7D4
                                        if (ch <= 0x1F77F) {// 1F774 .. 1F77F
                                            goto handle_extended_picto;
                                        } else {            // 1F780 .. 1F7D4
                                            goto handle_other;
                                        }
                                    }
                                }
                            }
                        } else {                            // 1F7D5 .. 1F88F
                            if (ch <= 0x1F847) {            // 1F7D5 .. 1F847
                                if (ch <= 0x1F80B) {        // 1F7D5 .. 1F80B
                                    if (ch <= 0x1F7FF) {    // 1F7D5 .. 1F7FF
                                        goto handle_extended_picto;
                                    } else {                // 1F800 .. 1F80B
                                        goto handle_other;
                                    }
                                } else {                    // 1F80C .. 1F847
                                    if (ch <= 0x1F80F) {    // 1F80C .. 1F80F
                                        goto handle_extended_picto;
                                    } else {                // 1F810 .. 1F847
                                        goto handle_other;
                                    }
                                }
                            } else {                        // 1F848 .. 1F88F
                                if (ch <= 0x1F859) {        // 1F848 .. 1F859
                                    if (ch <= 0x1F84F) {    // 1F848 .. 1F84F
                                        goto handle_extended_picto;
                                    } else {                // 1F850 .. 1F859
                                        goto handle_other;
                                    }
                                } else {                    // 1F85A .. 1F88F
                                    if (ch <= 0x1F85F) {    // 1F85A .. 1F85F
                                        goto handle_extended_picto;
                                    } else {                // 1F860 .. 1F88F
                                        if (ch <= 0x1F887) {// 1F860 .. 1F887
                                            goto handle_other;
                                        } else {            // 1F888 .. 1F88F
                                            goto handle_extended_picto;
                                        }
                                    }
                                }
                            }
                        }
                    } else {                                // 1F890 ..10FFFF
                        if (ch <= 0x2FFFD) {                // 1F890 .. 2FFFD
                            if (ch <= 0x1F9FF) {            // 1F890 .. 1F9FF
                                if (ch <= 0x1F8FF) {        // 1F890 .. 1F8FF
                                    if (ch <= 0x1F8AD) {    // 1F890 .. 1F8AD
                                        goto handle_other;
                                    } else {                // 1F8AE .. 1F8FF
                                        goto handle_extended_picto;
                                    }
                                } else {                    // 1F900 .. 1F9FF
                                    ch_width = 2_w;
                                    if (ch <= 0x1F90B) {    // 1F900 .. 1F90B
                                        goto handle_other;
                                    } else {                // 1F90C .. 1F9FF
                                        if (ch == 0x1F93B)
                                            goto handle_other;
                                        if (ch == 0x1F946)
                                            goto handle_other;
                                        goto handle_extended_picto;
                                    }
                                }
                            } else {                        // 1FA00 .. 2FFFD
                                if (ch <= 0x1FBFF) {        // 1FA00 .. 1FBFF
                                    if (ch <= 0x1FAFF) {    // 1FA00 .. 1FAFF
                                        goto handle_extended_picto;
                                    } else {                // 1FB00 .. 1FBFF
                                        goto handle_other;
                                    }
                                } else {                    // 1FC00 .. 2FFFD
                                    if (ch <= 0x1FFFD) {    // 1FC00 .. 1FFFD
                                        goto handle_extended_picto;
                                    } else {                // 1FFFE .. 2FFFD
                                        if (ch <= 0x1FFFF) {// 1FFFE .. 1FFFF
                                            goto handle_other;
                                        } else {            // 20000 .. 2FFFD
                                            ch_width = 2_w;
                                            goto handle_other;
                                        }
                                    }
                                }
                            }
                        } else {                            // 2FFFE ..10FFFF
                            if (ch <= 0xE001F) {            // 2FFFE .. E001F
                                if (ch <= 0x3FFFD) {        // 2FFFE .. 3FFFD
                                    if (ch <= 0x2FFFF) {    // 2FFFE .. 2FFFF
                                        goto handle_other;
                                    } else {                // 30000 .. 3FFFD
                                        ch_width = 2_w;
                                        goto handle_other;
                                    }
                                } else {                    // 3FFFE .. E001F
                                    if (ch <= 0xDFFFF) {    // 3FFFE .. DFFFF
                                        goto handle_other;
                                    } else {                // E0000 .. E001F
                                        goto handle_control;
                                    }
                                }
                            } else {                        // E0020 ..10FFFF
                                if (ch <= 0xE00FF) {        // E0020 .. E00FF
                                    if (ch <= 0xE007F) {    // E0020 .. E007F
                                        goto handle_extend_and_control;
                                    } else {                // E0080 .. E00FF
                                        goto handle_control;
                                    }
                                } else {                    // E0100 ..10FFFF
                                    if (ch <= 0xE01EF) {    // E0100 .. E01EF
                                        goto handle_extend;
                                    } else {                // E01F0 ..10FFFF
                                        if (ch <= 0xE0FFF) {// E01F0 .. E0FFF
                                            goto handle_control;
                                        } else {            // E1000 ..10FFFF
                                            goto handle_other;
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }